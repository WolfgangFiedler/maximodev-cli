<?xml version="1.0" encoding="UTF-8"?>
<product>
  <name>{{addon_id}}</name>
  <description>{{addon_description}}</description>
  <version>
    <major>{{version_major}}</major>
    <minor>{{version_minor}}</minor>
    <modlevel>{{version_modlevel}}</modlevel>
    <patch>{{version_patch}}</patch>
    <build>{{version_buildnumber}}</build>
  </version>
  <dbmaxvarname>{{maxvar_name}}</dbmaxvarname>
